{
    "compilerOptions": {
      "target": "ES2015",
      "module": "ES6",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }
  